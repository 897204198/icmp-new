{
    "name": "icmp",
    "version": "2.0.0-SNAPSHOT",
    "author": "ProperSoft",
    "description": "Ionic CMP APP",
    "private": true,
    "scripts": {
        "clean": "ionic-app-scripts clean",
        "lint": "ionic-app-scripts lint --bailOnLintError true",
        "build": "ionic-app-scripts build --prod --no-interactive",
        "build:prepare": "grunt build:before && npm run build",
        "build:hcp": "npm run build:prepare && grunt build:hcp",
        "build:releaseAndroid": "npm run build:prepare && grunt build:releaseAndroid",
        "build:releaseIOS": "npm run build:prepare && grunt build:releaseIOS",
        "grunt:releaseAndroid": "npm run build:prepare && grunt build:releaseAndroid",
        "grunt:releaseIOS": "npm run build:prepare && grunt build:releaseIOS",
        "debugAndroid": "npm run grunt:releaseAndroid && ionic cordova build android --prod --no-interactive",
        "releaseAndroid": "npm run grunt:releaseAndroid && ionic cordova build android --prod --no-interactive --release --buildConfig=build.json",
        "releaseIOS": "npm run grunt:releaseIOS && ionic cordova build ios --prod --no-interactive",
        "serve": "grunt serveProxy --force && ionic-app-scripts serve",
        "serve:real": "grunt serveProxy:real --force && ionic-app-scripts serve",
        "serve:win": "ionic-app-scripts serve",
        "e2e": "webdriver-manager update --standalone false --gecko false; protractor ./protractor.config.js"
    },
