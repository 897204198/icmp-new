{
  "name": "icmp",
  "version": "3.0.0",
  "author": "ProperSoft",
  "description": "Ionic CMP APP",
  "private": true,
  "scripts": {
    "bumpVer": "grunt bumpVer",
    "build": "ionic-app-scripts build --prod",
    "build:prepare": "grunt build:before && npm run build",
    "build:hcp": "npm run build:prepare && grunt build:hcp",
    "build:releaseAndroid": "npm run build:prepare && grunt build:releaseAndroid",
    "build:releaseIOS": "npm run build:prepare && grunt build:releaseIOS",
    "clean": "ionic-app-scripts clean",
    "debugAndroid": "npm run build:releaseAndroid && ionic cordova build android",
    "lint": "ionic-app-scripts lint",
    "releaseAndroid": "npm run build:releaseAndroid && ionic cordova build android --release --buildConfig=build.json",
    "releaseIOS": "npm run build:releaseIOS && ionic cordova build ios",
    "serve": "grunt serveProxy --force && ionic-app-scripts serve",
    "serve:real": "grunt serveProxy:real && ionic-app-scripts serve",
    "serve:win": "ionic-app-scripts serve",
    "build:e2e": "tsc -p e2e/",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "pree2e": "npm run build:e2e",
    "e2e": "webdriver-manager update --standalone false --gecko false; protractor ./protractor.config.js"
  },
  "dependencies": {
    "@angular/common": "4.1.3",
    "@angular/compiler": "4.1.3",
    "@angular/compiler-cli": "4.1.3",
    "@angular/core": "4.1.3",
    "@angular/forms": "4.1.3",
    "@angular/http": "4.1.3",
    "@angular/platform-browser": "4.1.3",
    "@angular/platform-browser-dynamic": "4.1.3",
    "@ionic-native/app-version": "^3.10.3",
    "@ionic-native/camera": "^4.1.0",
    "@ionic-native/core": "3.7.0",
    "@ionic-native/device": "^3.10.3",
    "@ionic-native/file": "4.0.0",
    "@ionic-native/file-transfer": "4.0.0",
    "@ionic-native/in-app-browser": "^3.12.1",
    "@ionic-native/splash-screen": "3.7.0",
    "@ionic-native/status-bar": "3.7.0",
    "@ionic-native/unique-device-id": "^3.10.3",
    "@ionic/storage": "^2.0.1",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^2.2.2",
    "@ngx-translate/core": "^7.0.0",
    "@ngx-translate/http-loader": "^0.1.0",
    "concurrently": "^3.5.0",
    "cordova-android": "^6.2.3",
    "cordova-ios": "^4.4.0",
    "cordova-plugin-advanced-http-2": "^1.6.1",
    "cordova-plugin-app-version": "^0.1.9",
    "cordova-plugin-compat": "^1.1.0",
    "cordova-plugin-console": "^1.0.7",
    "cordova-plugin-crosswalk-webview": "^2.3.0",
    "cordova-plugin-device": "^1.1.6",
    "cordova-plugin-file": "^4.3.3",
    "cordova-plugin-file-transfer": "1.6.3",
    "cordova-plugin-inappbrowser": "^1.7.1",
    "cordova-plugin-proper-properpush": "^1.1.1",
    "cordova-plugin-proper-update-version": "^1.0.2",
    "cordova-plugin-splashscreen": "^4.0.3",
    "cordova-plugin-statusbar": "^2.2.3",
    "cordova-plugin-uniquedeviceid": "^1.3.2",
    "cordova-plugin-whitelist": "^1.3.2",
    "cordova-plugin-x-toast": "^2.6.0",
    "cordova-sqlite-storage": "^2.0.4",
    "ionic-angular": "3.4.2",
    "ionic-plugin-keyboard": "^2.2.1",
    "ionicons": "3.0.0",
    "ng2-dragula": "^1.3.1",
    "platform": "^1.3.4",
    "rxjs": "5.4.0",
    "sw-toolbox": "^3.6.0",
    "ts-node": "^3.3.0",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@ionic/app-scripts": "^1.3.7",
    "@ionic/cli-plugin-cordova": "^1.4.0",
    "@ionic/cli-plugin-ionic-angular": "^1.3.1",
    "@types/jasmine": "^2.5.53",
    "canonical-path": "0.0.2",
    "grunt": "^0.4.5",
    "grunt-contrib-copy": "^0.7.0",
    "grunt-replace": "1.0.1",
    "grunt-shell": "^0.7.0",
    "jasmine-core": "^2.7.0",
    "jasmine-spec-reporter": "^4.1.1",
    "jit-grunt": "^0.9.1",
    "lite-server": "^2.3.0",
    "protractor": "^5.1.2",
    "protractor-jasmine2-html-reporter": "0.0.7",
    "time-grunt": "^1.0.0",
    "typescript": "2.2.1"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-console": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-whitelist": {},
      "ionic-plugin-keyboard": {},
      "cordova-plugin-crosswalk-webview": {
        "XWALK_VERSION": "23+",
        "XWALK_LITEVERSION": "xwalk_core_library_canary:17+",
        "XWALK_COMMANDLINE": "--disable-pull-to-refresh-effect",
        "XWALK_MODE": "embedded",
        "XWALK_MULTIPLEAPK": "true"
      },
      "cordova-plugin-x-toast": {},
      "cordova-sqlite-storage": {},
      "cordova-plugin-uniquedeviceid": {},
      "cordova-plugin-app-version": {},
      "cordova-plugin-proper-properpush": {},
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-proper-update-version": {}
    },
    "platforms": [
      "android",
      "ios"
    ]
  }
}
